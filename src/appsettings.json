{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "Serilog": {
        "Using": [
            "Sentry.Serilog",
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            {
                "Name": "Sentry",
                "Args": {
                    // Use SERILOG__WRITETO__0__ARGS__DSN environment variable to set DSN
                    // Use SERILOG__WRITETO__0__ARGS__ENVIRONMENT environment variable to set for env
                    // Replace double underscore with a colon if used in JSON override
                    "MinimumBreadcrumbLevel": "Information",
                    "MinimumEventLevel": "Error"
                }
            },
            {
                "Name": "Console"
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ],
        "Properties": {
            "Application": "Sample"
        }
    }
}